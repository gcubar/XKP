<xml>
    <class id="button" header="#include <QPushButton>" tag_name="QPushButton">
        <properties>
            <property name="caption" type="string" set_fn="text" get_fn="text" include_text="true"/>
            <property name="width" set_fn="width" get_fn="width" />
			<property name="height" set_fn="height" get_fn="height"/>
        </properties>
    </class>
    <class id="label" header="#include <QLabel>" tag_name="QLabel">
        <properties>
            <property name="caption" type="string" set_fn="text" get_fn="text" include_text="true"/>
            <property name="width" set_fn="width" get_fn="width" />
			<property name="height" set_fn="height" get_fn="height"/>
        </properties>
    </class>
    <class id="application" tag_name="div">
		<properties>
			<property name="title" type="string"/>
			<property name="width" type="int"/>
			<property name="height" type="int"/>
		</properties>
    </class>
</xml>
