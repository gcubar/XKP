<xml>
    <class id="camera" inst_class="THREE.Camera">
        <parameters>
            <parameter property_name="fov"/>
            <parameter property_name="aspect"/>
            <parameter property_name="near"/>
            <parameter property_name="far"/>     
			<parameter property_name="target"/>  			
        </parameters>
		<properties>
            <property name="position.x"  />
			<property name="position.y" />
			<property name="position.z" />
        </properties>
	</class>
	
	<class id="plane" inst_class="THREE.Plane">
        <parameters>
            <parameter property_name="width"/>
            <parameter property_name="height"/>
            <parameter property_name="segmentsWidth"/>
			<parameter property_name="segmentsHeight"/>
		</parameters>
		<properties>
            <property name="position.x"  />
			<property name="position.y" />
			<property name="position.z" />
			<property name="rotation.x"  />
			<property name="rotation.y" />
			<property name="rotation.z" />
        </properties>
	</class>
	
	<class id="cube" inst_class="THREE.Cube">
        <parameters>
            <parameter property_name="width"/>
            <parameter property_name="height"/>
            <parameter property_name="depth"/>      
			<parameter property_name="segmentsWidth"/>
			<parameter property_name="segmentsHeight"/>
			<parameter property_name="segmentsDepth"/>
			<parameter property_name="materials"/>
			<parameter property_name="flipped"/>
			<parameter property_name="sides"/>
        </parameters>
		<properties>
            <property name="position.x"  />
			<property name="position.y" />
			<property name="position.z" />
			<property name="rotation.x"  />
			<property name="rotation.y" />
			<property name="rotation.z" />
        </properties>
    </class>
	
	<class id="cylinder" inst_class="THREE.Cylinder">
        <parameters>
            <parameter property_name="numsegs"/>
            <parameter property_name="toprad"/>
            <parameter property_name="botrad"/>      
			<parameter property_name="height"/>
			<parameter property_name="topoffset"/>
			<parameter property_name="bottomoffset"/>
		</parameters>
		<properties>
            <property name="position.x"  />
			<property name="position.y" />
			<property name="position.z" />
			<property name="rotation.x"  />
			<property name="rotation.y" />
			<property name="rotation.z" />
        </properties>
    </class>
	
	<class id="sphere" inst_class="THREE.Sphere">
        <parameters>
            <parameter property_name="radius"/>
            <parameter property_name="segmentsWidth"/>
			<parameter property_name="segmentsHeight"/>
		</parameters>
		<properties>
            <property name="position.x"  />
			<property name="position.y" />
			<property name="position.z" />
			<property name="rotation.x"  />
			<property name="rotation.y" />
			<property name="rotation.z" />
        </properties>
    </class>
	
	<class id="face_material" inst_class="THREE.MeshFaceMaterial">
	</class>
	
	<class id="normal_material" inst_class="THREE.MeshNormalMaterial">
	</class>
	
	<class id="basic_material" inst_class="THREE.MeshBasicMaterial">
	</class>
	
	<class id="mesh" inst_class="THREE.Mesh">
		<parameters>
            <parameter property_name="geometry"/>
			<parameter property_name="materials"/>
		</parameters>
		<properties>
			<property name="position.x"  />
			<property name="position.y" />
			<property name="position.z" />
            <property name="rotation.x"  />
			<property name="rotation.y" />
			<property name="rotation.z" />
        </properties>
	</class>
	
	<class id="scene" inst_class="THREE.Scene">
		
	</class>
	
	<class id="texture">
		<parameters>
            <parameter property_name="color"/>
			<parameter property_name="wireframe"/>
			<parameter property_name="opacity"/>
			<parameter property_name="texture_src"/>
		</parameters>
	</class>

    <class id="application"/>
</xml>
